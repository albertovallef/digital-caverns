{
  "version": 2,
  "builds": [{
    "src": "build_files.sh",
    "use": "@vercel/static-build",
    "config": {
     "destDir": "staticfiles_build/static"
    }
  },
    {
    "src": "/digcaverns/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb"}
  }],
  "outputDirectory": "staticfiles_build/static",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "digcaverns/wsgi.py"
    }
  ]
}
