{
  "version": 2,
  "builds": [{
    "src": "digcaverns/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}
  },
  {
    "src": "build_files.sh",
    "use": "@vercel/static_build",
    "config": {
     "destDir": "staticfiles_build"
    }
  }],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "digcaverns/wsgi.py"
    }
  ]
}
